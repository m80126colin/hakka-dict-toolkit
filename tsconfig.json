{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "noImplicitAny": false,
    "declaration": true,
    "emitDeclarationOnly": true,
    "rootDir": "./lib",
    "outDir": "./dist",
    "watch": false,
    "lib": [ "es2015" ],
    "baseUrl": ".",
    "paths": {
      "@/*": [ "lib/*" ]
    }
  },
  "include": [
    "./lib/**/*"
  ]
}